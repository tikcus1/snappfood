    <children>
        <TextField fx:id="username_field" layoutX="14.0" layoutY="145.0" prefHeight="46.0" prefWidth="161.0" promptText="User name" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0 0 2 0;" />
        <Text layoutX="129.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register">
            <font>
                <Font size="45.0" />
            </font>
        </Text>
        <PasswordField fx:id="password_field" layoutX="14.0" layoutY="209.0" prefHeight="46.0" prefWidth="161.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0 0 2 0;" />
        <PasswordField fx:id="confirm_password_field" layoutX="14.0" layoutY="279.0" prefHeight="46.0" prefWidth="161.0" promptText="Confirm Password" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0 0 2 0;" />
